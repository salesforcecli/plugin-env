{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Environments",
  "definitions": {
    "Environments": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TableObject"
      }
    },
    "TableObject": {
      "type": "object",
      "additionalProperties": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "number"
          },
          {
            "type": "boolean"
          },
          {
            "type": "null"
          },
          {
            "not": {}
          }
        ]
      },
      "description": "Describes an object that is intended to be rendered in a table, which requires that no value is an array."
    }
  }
}
